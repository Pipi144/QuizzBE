{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "${DB_CONNECTION_STRING}"
  },
  "Auth0": {
    "Domain": "${AUTH0_DOMAIN}",
    "Audience": "${AUTH0_AUDIENCE}",
    "ClientId": "${AUTH0_CLIENT_ID}",
    "ClientSecret": "${AUTH0_CLIENT_SECRET}",
    "Connection": "${AUTH0_CONNECTION}"
  }

}
